{
  "name": "template-tanstack-start",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "prettier --check . && eslint --max-warnings 0 && vite build && tsc --noEmit && vitest run",
    "start": "PORT=$(scripts/port.ts) node -r dotenv/config .output/server/index.mjs",
    "format": "prettier --write .",
    "lint": "eslint --fix",
    "test": "vitest",
    "env:pull": "bunx vercel@latest env pull .env",
    "clean:soft": "rm -rf node_modules tsconfig.tsbuildinfo .nitro .output .tanstack dist && bun install",
    "clean:hard": "rm -rf node_modules tsconfig.tsbuildinfo .nitro .output .tanstack dist bun.lock && bun install"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-router": "^1.136.0",
    "@tanstack/react-router-devtools": "^1.136.0",
    "@tanstack/react-start": "^1.136.0",
    "@vercel/analytics": "^1.5.0",
    "nanoid": "^5.1.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.17",
    "tiny-invariant": "^1.3.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tanstack/eslint-plugin-router": "^1.133.19",
    "@tanstack/nitro-v2-vite-plugin": "^1.133.19",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.8"
  }
}
