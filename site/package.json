{
  "name": "template-tanstack-start",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "prettier --check . && eslint --max-warnings 0 && vite build && tsc --noEmit && vitest run",
    "start": "PORT=$(scripts/start.ts) node -r dotenv/config .output/server/index.mjs",
    "format": "prettier --write .",
    "lint": "eslint --max-warnings 0 --fix",
    "test": "vitest",
    "env:pull": "bunx vercel@latest env pull .env",
    "clean:soft": "rm -rf node_modules tsconfig.tsbuildinfo .nitro .output .tanstack && bun install",
    "clean:hard": "rm -rf node_modules tsconfig.tsbuildinfo .nitro .output .tanstack bun.lock && bun install"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/react-router": "^1.133.13",
    "@tanstack/react-router-devtools": "^1.133.13",
    "@tanstack/react-start": "^1.133.13",
    "@vercel/analytics": "^1.5.0",
    "nanoid": "^5.1.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.14",
    "tiny-invariant": "^1.3.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@tanstack/eslint-plugin-router": "^1.133.3",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "sharp": "^0.34.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.10",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
